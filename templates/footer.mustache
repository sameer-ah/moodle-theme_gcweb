{{!
    @template theme_gcweb/footer

    Page footer.
}}
        <footer id="wb-info">
            <div id="course-footer">{{{ output.course_footer }}}</div>
            <div class="landscape">
                <nav class="container wb-navcurr">
                    {{# footertext }}
                    <div class="footertext">
                        {{{ footertext }}}
                        <div style="clear:both"></div>
                    </div>
                    {{/ footertext }}
                    {{# showmoodledocs }}
                        {{# output.page_doc_link }}
                            <p class="helplink">{{{ output.page_doc_link }}}</p>
                        {{/ output.page_doc_link }}
                    {{/ showmoodledocs }}
                    {{# showhomelink }}
                        {{{ output.home_link }}}
                    {{/ showhomelink }}
                    {{# showlogininfo }}
                        {{{ output.login_info }}}
                    {{/ showlogininfo }}
                    {{{ output.standard_footer_html }}}
                    <div class="tool_usertours-resettourcontainer"></div>
                </nav>
            </div>
            <div class="brand">
                <div class="container">
                    <div class="row">
                        <nav class="col-md-10 ftr-urlt-lnk">
                        {{# footnote }}
                            <h2 class="wb-inv">{{# str }} footnoteheading, theme_gcweb {{/ str }}</h2>
                            <ul>
                                {{{ footnote }}}
                            </ul>
                        {{/ footnote }}
                        </nav>
                        <div class="col-xs-6 visible-sm visible-xs tofpg">
                            <a href="#wb-cont">{{# str }} topofpage, theme_gcweb {{/ str }} <span class="glyphicon glyphicon-chevron-up"></span></a>
                        </div>
                        <div class="col-md-2 col-xs-6 text-right">
                            <img src="{{{ wetboew }}}/assets/wmms-blk.svg" alt="{{# str }} symbol, theme_gcweb {{/ str }}" />
                        </div>
                    </div>
                </div>
            </div>
        </footer>
        {{#js}}
        require(['theme_boost/loader']);
        {{#shownavdrawer}}
        require(['theme_boost/drawer'], function(drawer) {
            drawer.init();
        });
        {{/shownavdrawer}}
        {{/js}}

        <!--[if gte IE 9 | !IE ]><!-->
            <script src="{{{ wetboew }}}/wet-boew/js/wet-boew.min.js"></script>
            <span id="wb-rsz" class="wb-init">&nbsp;</span>
        <!--<![endif]-->
        <!--[if lt IE 9]>
            <script src='{{{ wetboew }}}/wet-boew/js/ie8-wet-boew2.min.js'></script>
        <![endif]-->
        <script src="{{{ wetboew }}}/GCWeb/js/theme.min.js"></script>
        <script src="{{{ wetboew }}}/cdts/cdtscustom.js"></script>

        <div id="level-access-access-assistant-highlight-container" data-ae_invis="true"></div>
        {{#shownavdrawer}}
        {{> theme_gcweb/nav-drawer }}
        {{/shownavdrawer}}
        {{{ output.standard_end_of_body_html }}}
